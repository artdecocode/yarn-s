{
  "name": "yarn-s",
  "version": "0.0.0-pre",
  "description": "Run Multiple Yarn Commands In Series.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -t 5000 -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "dev": "node src/bin",
    "compile": "depack src/bin/yarn-s -o compile/bin/yarn-s.js -a -c -S --externs types/externs.js",
    "lib": "depack src/depack.js -o compile/yarn-s.js -a -c --source_map_include_content",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _yarnS",
    "wiki": "NODE_DEBUG=doc doc documentary-wiki -W wiki -n _yarnS",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u -t types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js",
    "pass": "node test/fixture/pass",
    "fail": "node test/fixture/fail",
    "args": "argufy -o src/bin/get-args.js"
  },
  "bin": {
    "yarn-s": "compile/bin/yarnS.js",
    "yarn-s-dev": "src/bin/index.js"
  },
  "files": [
    "src",
    "compile",
    "types/externs.js",
    "src/bin/index.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/yarn-s.git"
  },
  "keywords": [
    "yarn",
    "serial",
    "series"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/artdecocode/yarn-s/issues"
  },
  "homepage": "https://github.com/artdecocode/yarn-s#readme",
  "devDependencies": {
    "alamode": "^3.2.0",
    "argufy": "^1.7.1",
    "depack": "^1.0.0",
    "documentary": "^1.34.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.4.0",
    "spawncommand": "^2.2.0",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  }
}
